{
  "name": "@evilfactorylabs/gow",
  "description": "Simple URL shortener with simple analytics.",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "start": "run-p --print-name start:*",
    "start:bsb": "bsb -make-world -w -ws _",
    "start:web": "react-scripts start",
    "prebuild": "bsb -clean-world -make-world",
    "build": "react-scripts build",
    "clean": "bsb -clean-world"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "bs-bastet": "^1.2.5",
    "bs-fetch": "^0.6.2",
    "bs-webapi": "^0.17.1",
    "decco": "^1.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "reason-react": ">=0.9.1",
    "relude": "^0.63.1",
    "relude-fetch": "^0.8.0",
    "relude-reason-react": "^0.10.0"
  },
  "devDependencies": {
    "bs-platform": "^8.0.3",
    "npm-run-all": "^4.1.5"
  }
}
